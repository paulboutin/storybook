{
  "name": "botw-component-library",
  "version": "1.0.0",
  "scripts": {
    "start": "start-storybook -p ${PORT:-6006}",
    "build": "build-storybook",
    "lint": "pretty-quick",
    "test": "npm run test:server",
    "test:unit": "jest -c jest.config.js",
    "test:integration": "jest -c jest.integration.js",
    "test:server": "cross-env PORT=${PORT:-6006} start-server-and-test start http://localhost:${PORT:-6006} test:integration"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/preset-env": "7.10.2",
    "@storybook/html": "5.3.19",
    "@types/jest": "26.0.0",
    "@whitespace/storybook-addon-code": "1.0.2",
    "@whitespace/storybook-addon-html": "1.2.1",
    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "cross-env": "7.0.2",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "jest-image-snapshot": "4.0.2",
    "jest-puppeteer": "4.4.0",
    "node-sass": "4.14.1",
    "pretty-quick": "2.0.1",
    "puppeteer": "4.0.0",
    "sass-loader": "8.0.2",
    "start-server-and-test": "1.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
